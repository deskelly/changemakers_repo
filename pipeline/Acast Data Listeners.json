{
	"name": "Acast Data Listeners",
	"properties": {
		"activities": [
			{
				"name": "import csv data",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "import csv data",
						"type": "DataFlowReference"
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			},
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "import csv data",
						"dependencyConditions": [
							"Completed"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT\nrecordDate,\nrecordWeek,\nrecordMonth,\nrecordYear,\nlisteners\nFROM Acast_Listeners",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"recordDate"
							]
						},
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "recordDate",
									"type": "DateTime",
									"physicalType": "date"
								},
								"sink": {
									"name": "recordDate",
									"type": "DateTime",
									"physicalType": "date"
								}
							},
							{
								"source": {
									"name": "recordWeek",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "recordWeek",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "recordMonth",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "recordMonth",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "recordYear",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "recordYear",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "listeners",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "listeners",
									"type": "Int32",
									"physicalType": "int"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ChangeMakers_Staging",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "changemakersuat",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "DataIntegration/Acast"
		},
		"annotations": []
	}
}