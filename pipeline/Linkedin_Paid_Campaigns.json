								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"linkedServiceName": {
								"referenceName": "AzureSqlDatabase_Changemakers",
								"type": "LinkedServiceReference"
							},
							"typeProperties": {
								"scripts": [
									{
										"type": "Query",
										"text": "insert into Changemakers_Combined_Overview (channel,sub_channel, content, date, month, month_year, week, day_week, week_type, time_day, impressions, users,event_occurence,likes_consid_engage,shares,comments,follows,total_engagements,conversion_total_clicks,conversion_listened_watched,listened_watched_full_podcast_episode,activity_cost,cost_per_impression_cpm,cost_per_click_cpc)\n    SELECT \n        'Social Media' as 'channel'\n        ,'Linkedin Paid' as 'sub_channel'\n        , null\n\t\t, date as date\n        , datename(month, date) as month\n        , concat(datename(month, date), ' ', year(date)) as month_year\n        , concat(datename(month, date), ' ', year(date), ' Week ', (DATEPART(week, date) - DATEPART(week, DATEADD(day, 1, EOMONTH(date, -1)))) + 1) as week\n        , datename(dw, date) as day_week\n        , case when datename(dw, date) in ('Saturday', 'Sunday') then 'Weekend' else 'Weekday' end as week_type\n        , 'Unspecified' as time_day\n        , impressions\n        , impressions as users\n        , date as event_occurence\n        , reactions as likes_consid_engage\n        , shares\n        , comments\n        , follows\n        , (reactions + shares + comments + clicks) as total_engagements\n        , clicks as conversion_total_clicks\n        , video_views_25 as conversion_listened_watched\n        , video_completions as listened_watched_full_podcast_episode\n        , total_spend as activity_cost\n        , average_cpm as cost_per_impression_cpm\n        , average_cpc as cost_per_click_cpc\n    FROM Raw_LinkedinCampaign;"
									}
								],
								"scriptBlockExecutionTimeout": "02:00:00"
							}
