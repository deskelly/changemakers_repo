{
	"name": "Twitter_DataFlow",
	"properties": {
		"folder": {
			"name": "Twitter"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Twitter_GetFiles",
						"type": "DatasetReference"
					},
					"name": "SourceProcessFiles"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Twitter_PutFiles",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: true) ~> SourceProcessFiles",
				"SourceProcessFiles derive(each(match(true()), $$ = replace($$, '\",\"', '\";\"'))) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {Tweet id} as string,",
				"          {Tweet permalink} as string,",
				"          {Tweet text} as string,",
				"          time as string,",
				"          impressions as string,",
				"          engagements as string,",
				"          {engagement rate} as string,",
				"          retweets as string,",
				"          replies as string,",
				"          likes as string,",
				"          {user profile clicks} as string,",
				"          {url clicks} as string,",
				"          {hashtag clicks} as string,",
				"          {detail expands} as string,",
				"          {permalink clicks} as string,",
				"          {app opens} as string,",
				"          {app installs} as string,",
				"          follows as string,",
				"          {email tweet} as string,",
				"          {dial phone} as string,",
				"          {media views} as string,",
				"          {media engagements} as string,",
				"          {promoted impressions} as string,",
				"          {promoted engagements} as string,",
				"          {promoted engagement rate} as string,",
				"          {promoted retweets} as string,",
				"          {promoted replies} as string,",
				"          {promoted likes} as string,",
				"          {promoted user profile clicks} as string,",
				"          {promoted url clicks} as string,",
				"          {promoted hashtag clicks} as string,",
				"          {promoted detail expands} as string,",
				"          {promoted permalink clicks} as string,",
				"          {promoted app opens} as string,",
				"          {promoted app installs} as string,",
				"          {promoted follows} as string,",
				"          {promoted email tweet} as string,",
				"          {promoted dial phone} as string,",
				"          {promoted media views} as string,",
				"          {promoted media engagements} as string",
				"     ),",
				"     partitionFileNames:[('test1.csv')],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}