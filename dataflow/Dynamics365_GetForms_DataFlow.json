{
	"name": "Dynamics365_GetForms_DataFlow",
	"properties": {
		"folder": {
			"name": "Dynamics365"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Dynamics365_GetForms",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "substring"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {MessageId,PageId,FormId,FormName,FormSubmissionId,CustomerJourneyId,CustomerJourneyIterationId,Result,VisitorId,SessionId,ContactId,AccountId,ContactMatchingResult,LeadMatchingResult,CountryIsoCode,State,City,PostalCode,BrowserId,BrowserVersion,OperatingSystemId,OperatingSystemVersion,ReferrerUri,SendingId,SearchPhrase,SearchEngineName,VisitDuration,VisitorReturningStatus,ContactFailureMessage,LeadFailureMessage,Timestamp,VisitorAnonymousStatus,LeadId,SubmittedValues,WebsiteId,PageUrl,InternalMarketingInteractionId,OrganizationId,InteractionType,Version} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: true) ~> source1",
				"source1 derive(time_stamp = substring({MessageId,PageId,FormId,FormName,FormSubmissionId,CustomerJourneyId,CustomerJourneyIterationId,Result,VisitorId,SessionId,ContactId,AccountId,ContactMatchingResult,LeadMatchingResult,CountryIsoCode,State,City,PostalCode,BrowserId,BrowserVersion,OperatingSystemId,OperatingSystemVersion,ReferrerUri,SendingId,SearchPhrase,SearchEngineName,VisitDuration,VisitorReturningStatus,ContactFailureMessage,LeadFailureMessage,Timestamp,VisitorAnonymousStatus,LeadId,SubmittedValues,WebsiteId,PageUrl,InternalMarketingInteractionId,OrganizationId,InteractionType,Version}, (instr({MessageId,PageId,FormId,FormName,FormSubmissionId,CustomerJourneyId,CustomerJourneyIterationId,Result,VisitorId,SessionId,ContactId,AccountId,ContactMatchingResult,LeadMatchingResult,CountryIsoCode,State,City,PostalCode,BrowserId,BrowserVersion,OperatingSystemId,OperatingSystemVersion,ReferrerUri,SendingId,SearchPhrase,SearchEngineName,VisitDuration,VisitorReturningStatus,ContactFailureMessage,LeadFailureMessage,Timestamp,VisitorAnonymousStatus,LeadId,SubmittedValues,WebsiteId,PageUrl,InternalMarketingInteractionId,OrganizationId,InteractionType,Version}, '+00:00,') -20), 19)) ~> substring",
				"substring select(mapColumn(",
				"          time_stamp",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}