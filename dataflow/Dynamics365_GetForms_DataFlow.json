{
	"name": "Dynamics365_GetForms_DataFlow",
	"properties": {
		"folder": {
			"name": "Dynamics365"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Dynamics365_GetForms",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Dynamics365_Forms_AzureSqlTable",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          MessageId as string,",
				"          PageId as string,",
				"          FormId as string,",
				"          FormName as string,",
				"          FormSubmissionId as string,",
				"          CustomerJourneyId as string,",
				"          CustomerJourneyIterationId as string,",
				"          Result as string,",
				"          VisitorId as string,",
				"          SessionId as string,",
				"          ContactId as string,",
				"          AccountId as string,",
				"          ContactMatchingResult as string,",
				"          LeadMatchingResult as string,",
				"          CountryIsoCode as string,",
				"          State as string,",
				"          City as string,",
				"          PostalCode as string,",
				"          BrowserId as string,",
				"          BrowserVersion as string,",
				"          OperatingSystemId as string,",
				"          OperatingSystemVersion as string,",
				"          ReferrerUri as string,",
				"          SendingId as string,",
				"          SearchPhrase as string,",
				"          SearchEngineName as string,",
				"          VisitDuration as string,",
				"          VisitorReturningStatus as string,",
				"          ContactFailureMessage as string,",
				"          LeadFailureMessage as string,",
				"          Timestamp as string,",
				"          VisitorAnonymousStatus as string,",
				"          LeadId as string,",
				"          SubmittedValues as string,",
				"          WebsiteId as string,",
				"          PageUrl as string,",
				"          InternalMarketingInteractionId as string,",
				"          OrganizationId as string,",
				"          InteractionType as string,",
				"          Version as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: true) ~> source1",
				"derivedColumn1 select(mapColumn(",
				"          time_stamp,",
				"          content",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"source1 derive(time_stamp = substring(Timestamp, 0,19),",
				"          content = case(instr(PageUrl, 'changemakers') > 0, 'Changemakers', 'A&P')) ~> derivedColumn1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          date as string,",
				"          impressions as string,",
				"          reactions as string,",
				"          shares as string,",
				"          comments as string,",
				"          follows as string,",
				"          clicks as string,",
				"          click_through_rate as string,",
				"          video_view_rate as string,",
				"          video_views_25 as string,",
				"          video_completions as string,",
				"          video_completion_rate as string,",
				"          total_spend as string,",
				"          average_cpm as string,",
				"          average_cpc as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}