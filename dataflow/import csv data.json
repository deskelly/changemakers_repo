{
	"name": "import csv data",
	"properties": {
		"folder": {
			"name": "Acast File Imports"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Listeners_All_Time_Data_CSV",
						"type": "DatasetReference"
					},
					"name": "ListenersAllTimeDailyCSV"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ChangeMakers_Staging",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Date as string,",
				"          Listeners as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> ListenersAllTimeDailyCSV",
				"ListenersAllTimeDailyCSV derive(Date = toDate(Date, 'yyyy-MM-dd'),",
				"          Listeners = toInteger(Listeners)) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          recordDate as date,",
				"          listeners as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          recordDate = Date,",
				"          listeners = Listeners",
				"     )) ~> sink1"
			]
		}
	}
}