			"scriptLines": [
				"source(output(",
				"          LinkId as string,",
				"          ContactId as string,",
				"          AccountId as string,",
				"          LinkFriendlyName as string,",
				"          OriginalLink as string,",
				"          OriginalUrl as string,",
				"          MessageId as string,",
				"          SendingId as string,",
				"          EmailDomain as string,",
				"          EmailAddressUsed as string,",
				"          InteractionType as string,",
				"          OrganizationId as string,",
				"          Timestamp as string,",
				"          VisitorId as string,",
				"          SessionId as string,",
				"          CountryIsoCode as string,",
				"          State as string,",
				"          City as string,",
				"          PostalCode as string,",
				"          BrowserId as string,",
				"          BrowserVersion as string,",
				"          OperatingSystemId as string,",
				"          OperatingSystemVersion as string,",
				"          CustomerJourneyId as string,",
				"          CustomerJourneyIterationId as string,",
				"          UsageType as string,",
				"          ActivityId as string,",
				"          VisitorAnonymousStatus as string,",
				"          LeadId as string,",
				"          InternalMarketingInteractionId as string,",
				"          Version as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: true,",
				"     multiLineRow: true) ~> source1",
				"derivedColumn1 derive(time_stamp = substring({LinkId,ContactId,AccountId,LinkFriendlyName,OriginalLink,OriginalUrl,MessageId,SendingId,EmailDomain,EmailAddressUsed,InteractionType,OrganizationId,Timestamp,VisitorId,SessionId,CountryIsoCode,State,City,PostalCode,BrowserId,BrowserVersion,OperatingSystemId,OperatingSystemVersion,CustomerJourneyId,CustomerJourneyIterationId,UsageType,ActivityId,VisitorAnonymousStatus,LeadId,InternalMarketingInteractionId,Version}, instr({LinkId,ContactId,AccountId,LinkFriendlyName,OriginalLink,OriginalUrl,MessageId,SendingId,EmailDomain,EmailAddressUsed,InteractionType,OrganizationId,Timestamp,VisitorId,SessionId,CountryIsoCode,State,City,PostalCode,BrowserId,BrowserVersion,OperatingSystemId,OperatingSystemVersion,CustomerJourneyId,CustomerJourneyIterationId,UsageType,ActivityId,VisitorAnonymousStatus,LeadId,InternalMarketingInteractionId,Version}, '+00:00,') - 20, 19)) ~> substring",
				"substring select(mapColumn(",
				"          time_stamp",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"source1 derive(time_stamp = toDate(left(Timestamp,19),'yyyy-MM-dd mm:hh:ss')\r",
				"\r",
				") ~> derivedColumn1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          date as string,",
				"          impressions as string,",
				"          reactions as string,",
				"          shares as string,",
				"          comments as string,",
				"          follows as string,",
				"          clicks as string,",
				"          click_through_rate as string,",
				"          video_view_rate as string,",
				"          video_views_25 as string,",
				"          video_completions as string,",
				"          video_completion_rate as string,",
				"          total_spend as string,",
				"          average_cpm as string,",
				"          average_cpc as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}